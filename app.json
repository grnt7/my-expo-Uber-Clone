{
  "expo": {
    "name": "my-expo-Uber-Clone",
    "slug": "my-expo-Uber-Clone",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "my-expo-uber-clone", 
    "splash": {
      "image": "./src/images/Uber_Logo_Black.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.papaddog.myexpoUberClone",
      "config": {
        "googleMapsApiKey": "AIzaSy..." 
      }
    },
    "android": {
      "package": "com.papaddog.myexpoUberClone",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSy..."
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro",
      "output": "single"
    },
    "plugins": [
      "react-native-edge-to-edge"
    ],
    "extra": {
      "eas": {
        "projectId": "3daed313-56a5-4ed9-8426-f3feddac036c"
      }
    }
  }
}