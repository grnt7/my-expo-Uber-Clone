{
  "expo": {
    "name": "my-expo-Uber-Clone",
    "slug": "my-expo-Uber-Clone",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/Uber_Logo_Black_RGB.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff",
       "plugins": [
      "expo-router"
    ]
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.papaddog.myexpoUberClone"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
